<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    <context:annotation-config />
    
	<bean id="stu1" class="com.java.annotations.Student">
	    <property name="stuName">
	    <value>John</value>
	    </property>
	    <property name="age">
	    <value>20</value>
	    </property>
	</bean>
	
	<bean id = "trainer1" class="com.java.annotations.Trainer">
	<property name="trainerName">
	<value>"Harry"</value>
	</property>
	<property name ="trainerID">
	<value>123</value>
	</property>
	</bean>
	
	<bean id = "manager" class="com.java.annotations.Manager">
	<property name="managerName">
	<value>"David"</value>
	</property>
	<property name ="managerDept">
	<value>Java</value>
	</property>
	</bean>
			
	<bean id ="info" class = "com.java.annotations.Information"/>

    <!-- bean definitions go here -->	
	
</beans>