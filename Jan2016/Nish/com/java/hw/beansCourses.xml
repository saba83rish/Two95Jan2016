		<constructor-arg value="3 Months"></constructor-arg>
		<constructor-arg value="$2500"></constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="stu1" />
				<ref bean="stu2" />
				<ref bean="stu3" />
