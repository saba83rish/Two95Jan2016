<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  
   
<bean id = "trainer" class = "spring.example9.annotation.Trainer">
<property name="name" value="Ansari"></property>
</bean>

<bean id = "student" class = "spring.example9.annotation.Student">
<property name="name" value="Jisha"></property>
</bean>

<bean id = "hod" class = "spring.example9.annotation.Hod">
<property name="name" value="Sabrish"></property>
</bean>

<context:annotation-config/>
<bean id = "manager" class = "spring.example9.annotation.Manager"/>
</beans>



