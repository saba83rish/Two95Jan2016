<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="student1" class="com.two95.college.Student">
		<property name="studentName">
			<value>CJ</value>
		</property>
		<property name="studentId">
			<value>1</value>
		</property>
		<property name="courseAtt">
			<value>Java</value>
		</property>
	</bean>
	<bean id="student2" class="com.two95.college.Student">
		<property name="studentName">
			<value>Hri</value>
		</property>
		<property name="studentId">
			<value>2</value>
		</property>
		<property name="courseAtt">
			<value>Java</value>
		</property>
	</bean>
	<bean id="student3" class="com.two95.college.Student">
		<property name="studentName">
			<value>Ai</value>
		</property>
		<property name="studentId">
			<value>3</value>
		</property>
		<property name="courseAtt">
			<value>Java</value>
		</property>
	</bean>
	<bean id="student4" class="com.two95.college.Student">
		<property name="studentName">
			<value>CJT</value>
		</property>
		<property name="studentId">
			<value>4</value>
		</property>
		<property name="courseAtt">
			<value>.NETvalue</value>
		</property>
	</bean>
	<bean id="student5" class="com.two95.college.Student">
		<property name="studentName">
			<value>Hriya</value>
		</property>
		<property name="studentId">
			<value>5</value>
		</property>
		<property name="courseAtt">
			<value>.NET</value>
		</property>
	</bean>
	<bean id="student6" class="com.two95.college.Student">
		<property name="studentName">
			<value>Ais</value>
		</property>
		<property name="studentId">
			<value>6</value>
		</property>
		<property name="courseAtt">
			<value>.NET</value>
		</property>
	</bean>

	<bean id="trainer1" class="com.two95.college.Trainer">
		<property name="trainerName">
			<value>Ansari</value>
		</property>
		<property name="trainerId">
			<value>1</value>
		</property>
		<property name="courseHandled">
			<value>Java</value>
		</property>
	</bean>
	<bean id="trainer2" class="com.two95.college.Trainer">
		<property name="trainerName">
			<value>Sabarish</value>
		</property>
		<property name="trainerId">
			<value>1</value>
		</property>
		<property name="courseHandled">
			<value>Java</value>
		</property>
	</bean>
	<bean id="trainer3" class="com.two95.college.Trainer">
		<property name="trainerName">
			<value>Sam</value>
		</property>
		<property name="trainerId">
			<value>3</value>
		</property>
		<property name="courseHandled">
			<value>.NET</value>
		</property>
	</bean>
	<bean id="trainer4" class="com.two95.college.Trainer">
		<property name="trainerName">
			<value>Sabu</value>
		</property>
		<property name="trainerId">
			<value>4</value>
		</property>
		<property name="courseHandled">
			<value>.NET</value>
		</property>
	</bean>
	<bean id="course1" class="com.two95.college.Courses">
		<property name="courseName">
			<value>Java</value>
		</property>
		<property name="courseDuration">
			<value>6months</value>
		</property>
		<property name="courseFee">
			<value>6000</value>
		</property>
		<property name="trainer">
			<list>
				<ref bean="trainer1" />
				<ref bean="trainer2" />
			</list>
		</property>
		<property name="student">
			<list>
				<ref bean="student1" />
				<ref bean="student2" />
				<ref bean="student3" />
			</list>
		</property>
	</bean>
<bean id="course2" class="com.two95.college.Courses">
		<property name="courseName">
			<value>DOTNET</value>
		</property>
		<property name="courseDuration">
			<value>6months</value>
		</property>
		<property name="courseFee">
			<value>6000</value>
		</property>
		<property name="trainer">
			<list>
				<ref bean="trainer3" />
				<ref bean="trainer4" />
			</list>
		</property>
		<property name="student">
			<list>
				<ref bean="student4" />
				<ref bean="student5" />
				<ref bean="student6" />
			</list>
		</property>
	</bean>
	<bean id="college" class="com.two95.college.College">
		<constructor-arg value="two95Intl"></constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="course1" />
				<ref bean="course2" />
			</list>
		</constructor-arg>
	</bean>
</beans>  